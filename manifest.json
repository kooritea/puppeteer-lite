{
  "name": "PuppeteerLite",
  "description": "使用chrome扩展实现类似puppeteer的远程自动控制",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "dist/background/index.js",
    "type": "module"
  },
  "permissions": [
    "alarms",
    "activeTab",
    "tabs",
    "scripting",
    "storage",
    "debugger"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}